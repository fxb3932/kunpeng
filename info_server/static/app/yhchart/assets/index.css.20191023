.chart-title{
    font-size: 16px;
    text-align: left;
    font-family:'微软雅黑';
    font-weight:bold;
    padding: 10px 25px;
}
.rank-list{
    text-align: left;
    padding: 0 1px 3px;
    font-size: 10px;
}
.rank-list li{
    margin-bottom: 5px;
    height: 32px;
    line-height: 32px;
    background: -webkit-linear-gradient(left,rgba(6,25,53,0),rgba(6,25,53,0.1) 30%,rgba(6,25,53,0.3) 60%,rgba(6,25,53,.9));
    background: -o-linear-gradient(left,rgba(13,53,140,0),rgba(13,53,140,.9));
    background: -moz-linear-gradient(left,rgba(13,53,140,0),rgba(13,53,140,.9));
    background: linear-gradient(left,rgba(13,53,140,0),rgba(13,53,140,.9));
}
.rank-list li span:nth-child(1){
    width: 20px;
    height: 20px;
    line-height: 20px;
    display: inline-block;
    text-align: center;
    border-radius: 100%;
    color: #27EEFB;
}
/*17,21,84*/
.rank-list li:nth-child(1),.rank-list li:nth-child(2),.rank-list li:nth-child(3){
    background: -webkit-linear-gradient(left,rgba(36,111,192,0),rgba(36,111,192,0.1) 30%,rgba(36,111,192,0.3) 60%,rgba(36,111,192,.9));
    background: -o-linear-gradient(left,rgba(13,53,140,0),rgba(13,53,140,.9));
    background: -moz-linear-gradient(left,rgba(13,53,140,0),rgba(13,53,140,.9));
    background: linear-gradient(left,rgba(13,53,140,0),rgba(13,53,140,.9));
}
.rank-list li:nth-child(1) span:nth-child(3),.rank-list li:nth-child(2) span:nth-child(3),.rank-list li:nth-child(3) span:nth-child(3){
    color: #27EEFB;
}
.rank-list li:nth-child(1) span:nth-child(1){
    background: rgb(242,191,12);
    color: #222;
    box-shadow: 0 0 ;
}
.rank-list li:nth-child(2) span:nth-child(1){
    background: #3294ED;
    color: #222;
}
.rank-list li:nth-child(3) span:nth-child(1){
    background: #BE7B13;
    color: #222;
}
.rank-list li span:nth-child(2){
    padding-left: 10px;

}
.rank-list li span:nth-child(3){
    float: right;
    color: #1E61AD;
}
.rank-list li span:nth-child(3) i{
    float: right;
    transform: scale(.85);
    transform-origin: 20% 100%;
    color: #27EEFB;
    padding-right: 25px;
}

