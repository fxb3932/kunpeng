body, h1, h2, h3, h4, h5, h6, hr, p, blockquote, dl, dt, dd, ul, ol, li, pre, form, fieldset, legend, button, input, textarea, th, td{
    padding: 0;
    margin: 0;
    -moz-box-sizing: border-box;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
}
*:not([class*="icon"]):not(i){
    font-family: "Microsoft YaHei" !important;
}
*:before{
    -moz-box-sizing: border-box;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
}
*:after {
    -moz-box-sizing: border-box;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
}
input{
    -moz-appearance: none;
    -webkit-appearance: none;
    appearance: none;
    -moz-box-shadow: none;
    -webkit-box-shadow: none;
    box-shadow: none;
    border: none;
    outline: none;
    background:none;
}
textarea {
     -moz-appearance: none;
     -webkit-appearance: none;
     appearance: none;
     -moz-box-shadow: none;
     -webkit-box-shadow: none;
     box-shadow: none;
     border: none;
     outline: none;
     background:none;
}
select {
     -moz-appearance: none;
     -webkit-appearance: none;
     appearance: none;
     -moz-box-shadow: none;
     -webkit-box-shadow: none;
     box-shadow: none;
     border: none;
     outline: none;
     background:none;
}
textarea{
    resize:none
}
a {
    text-decoration: none;
}
a:-webkit-any-link {
    color: inherit;
}
ul{
    list-style: none
}
li{
     list-style: none
 }
ol{
     list-style: none
 }
i{
    font-style: normal;
}
em{
    font-style: normal;
}
h2 {
    -webkit-margin-before: 0;
    -webkit-margin-after: 0;
}
img{
    vertical-align: top;
}
.f-r{
    float: right;
}
.h-title{
    font-size: 18px;
    font-weight: normal;
    margin-bottom: 20px;
}
.font-s{
    font-size: 1.2rem;
}
