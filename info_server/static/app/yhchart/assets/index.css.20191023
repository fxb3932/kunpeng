.main{
    position: relative;
}
.bg{
    width: 65%;
    position: absolute;
    bottom: -14px;
    left: 17.5%;
    z-index: -1;
}
.page{
    position: relative;
}
.page .page-main{
    position: relative;
    width:1900px;
    top:10px;
    margin: 0 auto;
}
.top-title{
    position: relative;
    width: 30%;
    margin: 0 auto 20px;
    padding: 15px 130px 25px 225px;
    background: url("images/title-bg.png") center bottom no-repeat;
    background-size: 100% 20px;
}
.top-title .logo{
    position: absolute;
    top: 13px;
    left: 90px;
    width: 120px;
    float: left;
    margin-right: 15px;
}
.top-title .title{
    position: relative;
    font-size: 3rem;
    line-height: 3rem;
    float: left;
    padding-left: 15px;
}
.top-title .title:before{
    position: absolute;
    top: 0;
    left: 0;
    display: inline-block;
    content: '';
    width: 1px;
    height: 25px;
    background: rgba(255,255,255,.6);
}
.top-title .time{
    font-size: 2rem;
    float: right;
    padding-top: 5px;
}
.chart{
    position: relative;
    height: 400px;
    background: url("images/chart-bg.png") center no-repeat;
    background-size: 100% 100%;
}
.chartz{
    position: relative;
    height: 195px;
    background: url("images/chart-bg.png") center no-repeat;
    background-size: 100% 100%;
}
.chart-l{
    /*width: 57%;*/
    width: 100%;
    float: left;
}

.chart-lz{
    width: 100%;
    float: left;
}
.chart-r{
    width: 42%;
    float: right;
}
.chart-rz{
    width: 100%;
    float: left;
}
.chart-rl{
    width: 49%;
    float: left;
}
.chart-rr{
    width: 49%;
    float: right;
}
.chart-section{
    margin-top: 7px;
}

/*4个角*/
.main-left{
    position: relative;
    float: left;
    width: 34.5%;
}
.main-left:before,.main-left:after,.main-right:before,.main-right:after{
    position: absolute;
    content: '';
    display: inline-block;
}
.main-left:before{
    top: -5px;
    left: -10px;
    width: 100px;
    height: 200px;
    border-top: 2px solid #2868B0;
    border-left: 2px solid #2868B0;
    border-radius: 10px 0 0 0;
}
.main-right:before{
    top: -5px;
    right: -10px;
    width: 200px;
    height: 100px;
    border-top: 2px solid #2f868B0;
    border-right: 2px solid #2868B0;
    border-radius:0 10px 0 0;
}
.main-left:after{
    bottom: -10px;
    left: -10px;
    width: 200px;
    height: 100px;
    border-bottom: 2px solid #2868B0;
    border-left: 2px solid #2868B0;
    border-radius: 0 0 0 10px;
}
.main-right:after{
    bottom: -10px;
    right: -10px;
    width: 100px;
    height: 200px;
    border-bottom: 2px solid #2868B0;
    border-right: 2px solid #2868B0;
    border-radius: 0 0 10px 0;
}
.main-left .chart-l .chart-r{
    width: 41%;
}
.main-center{
    float: left;
    width: 30%;
    margin: 0 .5%;
}
.main-right{
    position: relative;
    float: right;
    width: 34.5%;
}
.main-right .chart-l .chart-r{
    width: 56%;
}
.main-right .chart-l{
    width: 42%;
}
.main-right .chart-r{
    width: 57%;
}
.chart-m{
    height: 450px;
}
.chart-s{
    height: 225px;
}
.margin-10{
    margin-top: 7px;
}
.num{
    height: 130px;
